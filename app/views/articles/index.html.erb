
      <!-- Main jumbotron for a primary marketing message or call to action -->
      <div class="jumbotron">
        <h1>Listing articles</h1>
        <table>
          <tr>
            <th>Title</th>
            <th>Text</th>
          </tr>
         
          <% @articles.each do |article| %>
            <tr>
              <td><%= article.title %></td>
              <td><%= article.text %></td>
            </tr>
          <% end %>
        </table>
        <p><%= link_to "Frontpage", root_path, class: "btn btn-primary btn-lg" %></p>
      </div>



